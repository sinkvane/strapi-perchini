{
  "kind": "collectionType",
  "collectionName": "restaurants",
  "info": {
    "singularName": "restaurant",
    "pluralName": "restaurants",
    "displayName": "Рестораны"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": false,
      "unique": false
    },
    "address": {
      "type": "string",
      "required": true
    },
    "PhoneNumber": {
      "type": "component",
      "component": "shared.phone-number",
      "repeatable": true
    },
    "menuUrl": {
      "type": "string"
    },
    "specialtyUrl": {
      "type": "string"
    },
    "socialLinks": {
      "type": "json"
    },
    "openHours": {
      "type": "string",
      "required": true
    },
    "businessLunch": {
      "type": "string"
    },
    "city": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::city.city",
      "inversedBy": "restaurants"
    }
  }
}
